<style>
/* HEADER STYLES */
  header {
    background: var(--bl-color-green-100);
    min-height: 80px;
    padding-block: var(--bl-spacing-2x-large);
  }

  .home {
    display: block;
    width: max-content;
    transition: color 250ms ease-in-out;

    &:hover,
    &:focus-visible {
      color: var(--bl-color-green-800);
    }
  }

  nav {
    /* mobile styles */
    display: flex;
    flex-direction: column;
    flex-wrap: nowrap;
    gap: var(--bl-spacing-2x-large);

    a {
      color: inherit;
      text-decoration: none;
      outline: none;
      transition: all 250ms ease-in-out;

      &:hover,
      &:focus-visible {
        color: var(--bl-color-green-800);
        text-decoration: underline;
        text-decoration-color: var(--bl-color-green-800);
        text-decoration-thickness: 4px;
        text-underline-offset: var(--bl-spacing-x-small);
        text-decoration-skip-ink: none;
      }
    }
  }

  ul {
    list-style: none;
    padding: 0;
    color: var(--bl-color-text-body);

    /* mobile styles */
    background: var(--bl-color-background-page);
    padding: var(--bl-spacing-2x-large);
    border-radius: var(--bl-border-radius-large);
    box-shadow: var(--bl-shadow-medium);

    display: flex;
    flex-direction: column;
    flex-wrap: nowrap;
    gap: var(--bl-spacing-x-large);
  }

  /* dont shrink/grow the FB icon */
  .social {
    align-self: start;
    flex: 0 0 32px;
    aspect-ratio: 1/1;
    transition: color 250ms ease-in-out;

    &:hover {
      color: var(--bl-color-green-800);
    }
  }


  @media (width > 600px) {


  }

  @media (width > 900px) {

    header {
      height: 80px;
      padding-block: unset;
    }

    nav {
      display: grid;
      gap: var(--bl-spacing-2x-large);
      grid-template-columns: 1fr 3fr;
      align-items: center;

      ul {
        /* larger screen styles */
        flex-direction: row;
        align-items: center;
        justify-self: flex-end;
        background: none;
        padding: 0;
        box-shadow: unset;
        border-radius: unset;
      }
    }

  }


</style>
